{
  "name": "logview",
  "version": "0.1.0",
  "description": "A tool for visualising Log JSON streams",
  "main": "main.js",
  "scripts": {
    "build": "lsc --no-header -c . && pug -o views views",
    "watch": "lsc --no-header -cw . & pug -wo views views",
