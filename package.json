    "view",
    "visualisation",
    "visualization",
    "journal",
    "ledger",
    "visualizer",
    "visualiser",
    "json",
    "stream",
    "logify"
  ],
  "author": "Yousef Amar <yousef@amar.io> (http://yousefamar.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yousefamar/logview/issues"
  },
  "homepage": "https://github.com/yousefamar/logview#readme",
  "devDependencies": {
    "electron": "^1.4.15",
    "livescript": "^1.5.0",
    "pug-cli": "^1.0.0-alpha6"
  }
}
